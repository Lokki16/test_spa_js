<% for (var i = 0; i < products.length; ++i) { %>
<tr>
    <td><img src="<%= products[i]['media']%>" height="50"></td>
    <td><%= products[i]['name'] %></td>
    <td><%= products[i]['status'] %></td>
    <td><%= products[i]['price'] %></td>
    <td>
        <a class="btn" href="/products/edit?id=<%= products[i]._id%>"><i class="fa fa-gear"></i></a>
        <a class="btn"><i class="fa fa-times"></i></a>
    </td>
</tr>
<% } %>
